{
  "name": "unimask",
  "version": "1.0.0",
  "type": "module",
  "description": "A lightweight utility library for creating mask for input values",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "vue": [
        "./dist/vue/index.d.ts"
      ],
      "react": [
        "./dist/react/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "biome check",
    "lint:fix": "pnpm run lint -- --write",
    "prepare": "husky",
    "husky:pre-commit": "pnpm run lint",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@anolilab/semantic-release-pnpm": "^1.1.10",
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@types/node": "^22.13.11",
    "@vitest/coverage-v8": "^3.0.8",
    "@vue/test-utils": "^2.4.6",
    "biome": "^0.3.3",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "is-ci": "^4.1.0",
    "react": "^19.0.0",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8",
    "vue": "^3.5.13"
  },
  "files": [
    "dist/*",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "mask",
    "vue-mask",
    "formatting",
    "vue-formatting",
    "input",
    "string",
    "masking"
  ],
  "author": "dschewchenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dschewchenko/unimask/issues"
  },
  "homepage": "https://github.com/dschewchenko/unimask#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dschewchenko/unimask.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@anolilab/semantic-release-pnpm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "esbuild"
    ]
  }
}
